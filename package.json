    "eslint-config-next": "15.3.0",
    "husky": "^9.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "local-ssl-proxy": "^2.0.5",
    "postcss": "^8.5.0",
    "prettier": "^3.4.2",
    "tailwindcss": "^4.0.0",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0"
  }
