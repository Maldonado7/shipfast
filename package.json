    "swagger-ui-react": "^5.21.0",
    "tailwind-merge": "^2.6.0",
    "uploadthing": "^7.7.1",
    "workbox-webpack-plugin": "^7.2.0",
    "zod": "^3.24.1",
    "zustand": "^5.0.2"
