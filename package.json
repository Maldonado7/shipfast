{
  "name": "shipfast-template",
  "version": "1.0.0",
  "description": "ðŸš€ The most comprehensive, production-ready Next.js starter template. Built with TypeScript, Supabase, modern authentication, real-time features, and everything you need to ship fast.",
  "keywords": ["nextjs", "react", "typescript", "supabase", "tailwindcss", "template", "starter", "fullstack", "authentication", "realtime"],
  "homepage": "https://shipfast-template.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/Maldonado7/Template.git"
  },
  "bugs": {
    "url": "https://github.com/Maldonado7/Template/issues"
